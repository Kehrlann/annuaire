{
  "name": "boilerplate-backbone-react-browserify",
  "version": "0.0.1",
  "description": "A boilerplate project with backbone, react and browserify",
  "main": "app/Router.js",
  "author": "",
  "license": "",
  "dependencies": {
    "jquery": "^2.1.0",
    "backbone": "^1.1.2",
    "underscore": "*",
    "bootstrap": "^3.2.0",
    "react-tools": "^0.12.1",
	"react": "^0.12.1"
  },
  "devDependencies": {
    "beefy": "^1.1.0",
    "browserify": "^3.44.2",
    "reactify": "^0.17.1",
    "envify": "^3.2.0",
    "uglify-js": "^2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "watchify -o js/app-bundle.js -v -d .",
    "build": "NODE_ENV=production browserifyb . | uglifyjs -cm > app-bundle.min.js",
    "collect-static": "collect-static . ./static",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}

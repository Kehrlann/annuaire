{
  "name": "boilerplate-backbone-react-browserify",
  "version": "0.0.1",
  "description": "A boilerplate project with backbone, react and browserify",
  "main": "app/Router.js",
  "author": "",
  "license": "",
  "dependencies": {
    "jquery": "^2.1.0",
    "backbone": "^1.1.2",
    "underscore": "*",
    "twitter-bootstrap": "^2.1.1",
    "bootstrap-browserify": "^2.1.1"
  },
  "devDependencies": {
    "browserify": "^3.44.2",
    "beefy": "^1.1.0"
  },
  "scripts": {
    "start": "STATIC_ROOT=./static watchify -o app-bundle.js -v -d .",
    "build": "STATIC_ROOT=./static NODE_ENV=production browserify . | uglifyjs -cm > app-bundle.min.js",
    "collect-static": "collect-static . ./static",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
